function tunnel(new_q){
    return e.questions_json[e.questions_json.map(f=>f.pk).indexOf(new_q)];
}

cyoAdventure = function(a) {
    ans = campaignTrail_temp.player_answers[campaignTrail_temp.player_answers.length-1];
    e.noCounter += 1

    //win
    if (ans == 83985 || ans == 83986 || ans == 83991 || ans == 83992 || ans == 83996 || ans == 83997 || ans == 84000 || ans == 84009 || ans == 84010 || ans == 84013 || ans == 84015 || ans == 84036 || ans == 84037) {
        win += 1;
        updateStatsPopup();
    }

    //progrep
    if (ans == 83952 || ans == 83962 || ans == 83985 || ans == 83990 || ans == 83996 || ans == 84004 || ans == 84009 || ans == 84013 || ans == 84017 || ans == 84036) {
        progrep += 1;
        updateStatsPopup();
    }

    if (ans == 83995 || ans == 83999 || ans == 84018) {
        progrep -= 1;
        updateStatsPopup();
    }

    if (ans == 84038) {
        progrep -= 2;
        updateStatsPopup();
    }

    //trust
    if (ans == 83952 || ans == 83957 || ans == 83985 || ans == 83986 || ans == 83988 || ans == 83990 || ans == 83991 || ans == 83996 || ans == 84000 || ans == 84004 || ans == 84009 || ans == 84010 || ans == 84013 || ans == 84015 || ans == 84017 || ans == 84036 || ans == 84037 || ans == 84038) {
        trust += 1;
        updateStatsPopup();
    }

    if (ans == 83995 || ans == 84002 || ans == 84005 || ans == 84011 || ans == 84014) {
        trust -= 1;
        updateStatsPopup();
    }

    if (ans == 84018) { 
        trust -= 2; 
        updateStatsPopup();
    }
    
    //did you debate
    if (ans == 84073){
        debate += 1;
    }

    //running
    if (ans == 84017){
        running = false;
    }

    //Q10
    if (e.noCounter == 9) {
        if (running == true){
            if (win > 8){
                campaignTrail_temp.questions_json[9] = tunnel(84039); 
            } else if (win >= 6){
                campaignTrail_temp.questions_json[9] = tunnel(84040); 
            } else if (win < 6){
                campaignTrail_temp.questions_json[9] = tunnel(84041); 
            }
        } else if (running == false){
            if (win > 6){
                campaignTrail_temp.questions_json[9] = tunnel(84039); 
            } else if (win >= 4){
                campaignTrail_temp.questions_json[9] = tunnel(84040); 
            }else if (win < 4){
                campaignTrail_temp.questions_json[9] = tunnel(84041); 
            }
        }
    }

    //Q12
    if (e.noCounter == 11){
        if(running == true){
            campaignTrail_temp.questions_json[11] = tunnel(84042); 
        }else if(running == false){
            campaignTrail_temp.questions_json[11] = tunnel(84050); 
        }
    }

    //Q13
    if (e.noCounter == 12){
        if(running == true){
            if(e.player_answers.includes(84073)){
                campaignTrail_temp.questions_json[12] = tunnel(84043); 
            } else if (e.player_answers.includes(84074)){
                campaignTrail_temp.questions_json[12] = tunnel(84044);
            }
        }else if(running == false){
            if(e.player_answers.includes(84036)||e.player_answers.includes(84037)){
                campaignTrail_temp.questions_json[12] = tunnel(84114);
            } else if(e.player_answers.includes(84038)){
                campaignTrail_temp.questions_json[12] = tunnel(84115);
            }
        }
    }

    //Q14
    if (e.noCounter == 13) {
        if (win >= 4) {
            if (trust >= 6) {
                switcheroo29(); 
                switcheroo32(); 
            } else if (trust < 6) {
                switcheroo28(); 
                switcheroo34(); 
            }
        } else if (win < 4) {
            if (trust >= 6) {
                switcheroo33(); 
                switcheroo30(); 
            } else if (trust < 6) {
                switcheroo35(); 
                switcheroo31(); 
            }
        }

        if (running == true) {
            if (debate == 1 && win > 6 && trust > 5) { // stuck in after debate
                campaignTrail_temp.questions_json[13] = tunnel(84047);
                switcheroo21();
                vpTable["Donald Trump"] = "Doug Burgum";
                poss += 1;
            } else if (debate == 1) { // forced out after debate
                campaignTrail_temp.questions_json[13] = tunnel(84046);
                switcheroo1();
                switcheroo11();
                forced = true;
                poss += 1;
            } else if (debate == 0 && trust > 5) {
                if (e.player_answers.includes(84086)) { // rose garden stuck in 
                    campaignTrail_temp.questions_json[13] = tunnel(84047);
                    switcheroo23();
                } else if (e.player_answers.includes(84120)) { // active campaign stuck in
                    campaignTrail_temp.questions_json[13] = tunnel(84047);
                    switcheroo26();
                    vpTable["Donald Trump"] = "Tim Scott";
                }
            } else if (debate == 0) {
                if (e.player_answers.includes(84086)) { // rose garden forced out
                    campaignTrail_temp.questions_json[13] = tunnel(84046);
                    switcheroo2();
                    switcheroo24();
                    vpTable["Kamala Harris"] = "Josh Shapiro";
                    vpTable["Glenn Youngkin"] = "Tom Emmer";
                    forced = true;
                } else if (e.player_answers.includes(84120)) { // active campaign forced out
                    campaignTrail_temp.questions_json[13] = tunnel(84046);
                    switcheroo3();
                    switcheroo25();
                    vpTable["Kamala Harris"] = "Andy Beshear";
                    vpTable["Kari Lake"] = "Josh Hawley";
                    forced = true;
                }
            }
        } 
        else if (running == false) {
            campaignTrail_temp.questions_json[13] = tunnel(84045);
            
            if (progrep >= 5) {
                if (win >= 4) {
                    if (trust >= 6) {
                        switcheroo4();
                        switcheroo12();
                    } else if (trust < 6) {
                        switcheroo5();
                        switcheroo13();
                    }
                } else if (win < 4) {
                    if (trust >= 6) {
                        switcheroo3();
                        switcheroo14();
                        vpTable["Kamala Harris"] = "Andy Beshear";
                    } else if (trust < 6) {
                        switcheroo6();
                        switcheroo15();
                    }
                }
            } else if (progrep < 5) {
                if (win >= 4) {
                    if (trust >= 6) {
                        switcheroo7();
                        switcheroo16();
                    } else if (trust < 6) {
                        switcheroo8();
                        switcheroo17();
                    }
                } else if (win < 4) {
                    if (trust >= 6) {
                        switcheroo9();
                        switcheroo18();
                    } else if (trust < 6) {
                        switcheroo10();
                        switcheroo19();
                        vpTable["Donald Trump"] = "Sarah Huckabee Sanders";
                    }
                } 
            }
        }
    }

    //Q15
    if (e.noCounter == 14){
        if(running == false){
            if (progrep >= 5){
                if(win >= 6){
                    if(trust >= 9){
                        campaignTrail_temp.questions_json[14] = tunnel(84049);
                    }else if (trust < 9){                        
                        campaignTrail_temp.questions_json[14] = tunnel(84051);
                    }
                } else if(win < 6){
                    if(trust >= 9){
                        campaignTrail_temp.questions_json[14] = tunnel(84052);
                    } else if (trust < 9){
                        campaignTrail_temp.questions_json[14] = tunnel(84053);
                    }
                }
            }else if (progrep < 5){
                if(win >= 6){
                    if(trust >= 9){
                        campaignTrail_temp.questions_json[14] = tunnel(84054);
                    }else if (trust < 9){
                        campaignTrail_temp.questions_json[14] = tunnel(84055);
                    }
                } else if(win < 6){
                    if(trust >= 9){
                        campaignTrail_temp.questions_json[14] = tunnel(84056);
                    } else if (trust < 9){
                        campaignTrail_temp.questions_json[14] = tunnel(84057);
                    }
                } 
            }
        }else if(running == true){
            if(poss == 1 && trust < 2 && win > 6 && forced == false){ //assassination
                campaignTrail_temp.questions_json[14] = tunnel(84048);
                kill += 1;
                switcheroo22();
                vpTable["Doug Burgum"] = "Marco Rubio";
            }else if(poss == 1 && trust < 2 && forced == true){ //assassination
                campaignTrail_temp.questions_json[14] = tunnel(84048);
                kill += 1;
                switcheroo20();
            }else if (debate == 1 && forced == true){
                campaignTrail_temp.questions_json[14] = tunnel(84125);
            }else if (debate == 1 && forced == false){
                campaignTrail_temp.questions_json[14] = tunnel(84127);
            } else if (debate == 0 && trust > 5) {
                if (e.player_answers.includes(84086)) { // rose garden stuck in 
                    campaignTrail_temp.questions_json[14] = tunnel(84058);
                } else if (e.player_answers.includes(84120)) { // active campaign stuck in
                    campaignTrail_temp.questions_json[14] = tunnel(84129);
                }
            } else if (debate == 0) {
                if (e.player_answers.includes(84086)) { // rose garden forced out
                    campaignTrail_temp.questions_json[14] = tunnel(84059);
                } else if (e.player_answers.includes(84120)) { // active campaign forced out
                    campaignTrail_temp.questions_json[14] = tunnel(84060);
                }
            }
        }
    }

    //Q16
    if(e.noCounter == 15){
        if(kill == 1){
            campaignTrail_temp.questions_json[15] = tunnel(84064);
        }else if(running == true && forced == true){
            campaignTrail_temp.questions_json[15] = tunnel(84065);
        }else if(running == true && forced == false){
            campaignTrail_temp.questions_json[15] = tunnel(84066);
        }else if(running == false){
            campaignTrail_temp.questions_json[15] = tunnel(84066);
        }
    }

    //Q17
    if(e.noCounter == 16){
        if(running == false || (running == true && forced == true)){
            if(trust > 7){
                campaignTrail_temp.questions_json[16] = tunnel(84070);
            }else {
                campaignTrail_temp.questions_json[16] = tunnel(84069);
            }
        }else if(running == true && forced == false){
            if(trust > 7){
                campaignTrail_temp.questions_json[16] = tunnel(84068);
            }else {
                campaignTrail_temp.questions_json[16] = tunnel(84067);
            }
        }
    }
}

