HistHexcolour=["#F07763","#698DC5","#17aa5c","#fed105"];
HistName=["⠀Donald Trump","⠀Kamala Harris","⠀Jill Stein","⠀Chase Oliver"];
HistEV=[312,226,0,0];
HistPV=["77,302,580","75,017,613","862,049","650,126"];
HistPVP=["49.8%","48.3%","0.6%","0.4%"];

function onGameWindowChangeCandidates() {
    const resultsTableHolder = document.getElementById("overall_details_container");

    if(!resultsTableHolder || resultsTableHolder.classList.contains("done")) {
        return;
    }

    resultsTableHolder.classList.add("done");

    const resultsTable = resultsTableHolder.childNodes[3].childNodes[3].children[0];
    
    for(let i = 0; i < resultsTable.children.length; i++) {
        const row = resultsTable.children[i];
        let name = row.firstChild.innerText;
        row.insertCell(1);
        if(name) {
            name = name.replaceAll("-", "").trim();
            console.log("name is:" + name)
        }

        if(i == 0) {
            row.children[1].innerHTML = `<span style="font-weight:bold">Running Mate<span>`;
        }
        else {
            row.children[1].innerHTML = vpTable[name];
        }
    }

    
    const resultsTableHistorical = resultsTableHolder.childNodes[5].childNodes[3].children[0];
    
    for(let i = 0; i < resultsTableHistorical.children.length; i++) {
        const row = resultsTableHistorical.children[i];
        let name = row.childNodes[1].innerText;
        row.insertCell(1);
        if(name) {
            name = name.replaceAll("-", "").trim();
        }

        if(i == 0) {
            row.children[1].innerHTML = `<span style="font-weight:bold">Running Mate<span>`;
        }
        else {
            row.children[1].innerHTML = vpTableHistorical[name];
        }
    }


    // Add CSS to increase row spacing
    const styleElem = document.createElement('style');
    styleElem.textContent = `
        #overall_details_container table tr {
            line-height: 1.125; 
        }
    `;
    document.head.appendChild(styleElem);
}

const gameWindowNode = document.getElementById("game_window");
const candidateWatcherConfig = { attributes: true, childList: true, subtree: true };

const candidateObserver = new MutationObserver(onGameWindowChangeCandidates);
candidateObserver.observe(gameWindowNode, candidateWatcherConfig);

const box = document.createElement('div');
box.id = 'draggable-box';
box.className = 'game-stats-popup';

Object.assign(box.style, {
    position: "fixed",
    bottom: "20px",
    right: "20px",
    width: "140px",
    height: "140px",
    backgroundImage: "url('https://i.imgur.com/xZzt24H.png')", 
    backgroundSize: "cover",
    backgroundPosition: "center",
    color: "#FFFFFF",
    padding: "10px",
    boxShadow: "0 0 10px rgba(0, 0, 0, 0.3)",
    zIndex: "10000",
    fontSize: "14px",
    display: "block",
    cursor: "move",
    overflowY: "visible",
    fontFamily: "'Segoe UI', Arial, sans-serif"
});

const content = document.createElement('div');
box.appendChild(content);

document.body.appendChild(box);

let isDragging = false;
let offsetX = 0, offsetY = 0;

box.addEventListener('mousedown', (e) => {
    isDragging = true;
    offsetX = e.clientX - box.getBoundingClientRect().left;
    offsetY = e.clientY - box.getBoundingClientRect().top;
    box.style.transition = 'none';
});

document.addEventListener('mousemove', (e) => {
    if (isDragging) {
        box.style.left = `${e.clientX - offsetX}px`;
        box.style.top = `${e.clientY - offsetY}px`;
        box.style.right = 'auto';
        box.style.bottom = 'auto';
    }
});

document.addEventListener('mouseup', () => {
    isDragging = false;
    box.style.transition = 'all 0.1s ease';
});


// Update the injectTooltipStyles function
function injectTooltipStyles() {
    if (document.getElementById("tooltipStyles")) return;

    const style = document.createElement("style");
    style.id = "tooltipStyles";
    style.textContent = `
        /* Previous tooltip styles remain the same */
        .hover-value {
            border-bottom: 1px dotted #fff;
            cursor: help;
            position: relative;
        }

        .hover-value::after {
            content: attr(data-tooltip);
            position: absolute;
            background: rgba(0, 0, 0, 0.8);
            color: #FFFFFF;
            padding: 6px 10px;
            font-size: 13px;
            font-family: 'Segoe UI', sans-serif;
            border-radius: 5px;
            box-shadow: 0 0 5px rgba(0,0,0,0.5);
            white-space: nowrap;
            opacity: 0;
            pointer-events: none;
            top: -32px;
            left: 50%;
            transform: translateX(-50%);
            z-index: 10001;
            transition: opacity 0.2s ease-in-out;
        }

        .hover-value:hover::after {
            opacity: 1;
        }
        
        .stat-row {
            margin: 10px 0;
            padding: 0px;
            line-height: 1.2;
            transition: background 0.2s ease;
            border-radius: 4px;
            color: black;
        }

    `;
    document.head.appendChild(style);
}

const VARIABLE_DESCRIPTIONS = {
    'win': 'Major victories achieved during term',
    'progrep': 'Appeal to progressives',
    'trust': 'Public trust and approval ratings'
};

function createScoreElement(label, value, container) {
    injectTooltipStyles();
    
    let statusMessage;
    switch(label.toLowerCase()) {
        case 'win':
            statusMessage = value > 8 ? `Historic achievement` :
                          value > 3 ? `Solid performance` :
                          `Needs more wins`;
            break;
        case 'progrep':
            statusMessage = value > 7 ? `Perfect progressive-realism balance` :
                          value > 4 ? `Moderate balance` :
                          `Needs better alignment`;
            break;
        case 'trust':
            statusMessage = value > 7 ? `Sky-high public trust` :
                          value > 4 ? `Moderate approval` :
                          `Low confidence`;
            break;
        default:
            statusMessage = `${value}`;
    }

    const row = document.createElement('div');
    row.className = 'stat-row';
    row.innerHTML = `
        <span class="hover-value" data-tooltip="${VARIABLE_DESCRIPTIONS[label.toLowerCase()] || label}">
            ${label}
        </span>: 
        <span class="hover-value" data-tooltip="${statusMessage}">
            ${value}
        </span>
    `;
    
    container.appendChild(row);
}

function updateStatsPopup() {
    content.innerHTML = '';
    createScoreElement('Win', win, content);
    createScoreElement('ProgRep', progrep, content);
    createScoreElement('Trust', trust, content);
}

window.updateStatsPopup = updateStatsPopup;
updateStatsPopup();


function addStatsPopupButton() {
    if (document.getElementById('stats_popup_button')) return;

    // Find the reference button (electoral map button)
    const refButton = document.getElementById('view_electoral_map');
    if (!refButton) {
        console.warn('Could not find electoral map button');
        return;
    }

    // Create the stats button
    const newButton = document.createElement('button');
    newButton.id = 'stats_popup_button';
    newButton.style.cssText = `
        margin-left: 1.5em;
        position: static !important;
        display: inline-block !important;
    `;
    newButton.textContent = 'Game Stats';

    refButton.insertAdjacentElement('afterend', newButton);
    newButton.addEventListener('click', toggleStatsPopup);
}

function toggleStatsPopup() {
    const box = document.getElementById('draggable-box');
    if (box.style.display === 'none' || !box.style.display) {
        box.style.display = 'block';
    } else {
        box.style.display = 'none';
    }
}

function handleMutations(mutationsList, observer) {
    if (observerRunning) return;
    observerRunning = true;

    addStatsPopupButton();

    observer.observe(document.documentElement, { childList: true, subtree: true });
    observerRunning = false;
}


let observerRunning = false;

var element = document.getElementById('controlElement');
if (!element) {
    let singleObserver = new MutationObserver(handleMutations);
    singleObserver.observe(document.documentElement, { childList: true, subtree: true });
    var controlElement = document.createElement('div');
    controlElement.style.display = 'none';
    controlElement.id = 'controlElement';
    document.body.appendChild(controlElement);
}
