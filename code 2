//dems
//harris/walz
function switcheroo1() {
    var id = e.candidate_json.map(p=>p.pk).indexOf(201)
    e.candidate_json[id].fields.first_name = "Kamala"
    e.candidate_json[id].fields.last_name = "Harris"
    e.candidate_json[id].fields.color_hex = "#698DC5" 
}
